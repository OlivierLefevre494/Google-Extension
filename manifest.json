{
    "manifest_version": 3,
    "name": "Website blocker!",
    "description": "A website blocker for higher productivity!",
    "version": "1.0",
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "host_permissions": [
      "http://*/*",
      "https://*/*"
    ],
    "permissions": [
      "tabs",
      "scripting",
      "storage",
      "unlimitedStorage"
    ],
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": "Hourglass_icon.png"
    }
  }